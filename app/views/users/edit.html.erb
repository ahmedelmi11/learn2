<div class="container">
  <div class="row">
    <div class="col-xs-6 col-xs-offset-3">
      <h1>Edit profile</h1>
        <%= simple_form_for(@user) do |f| %>
          <%= f.input :first_name %>
          <%= f.input :last_name %>
          <%= f.input :email %>
          <%= f.submit %>
        <% end %>
    </div>
  </div>
</div>
